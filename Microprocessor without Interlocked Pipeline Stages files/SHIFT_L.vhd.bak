LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;
USE IEEE.NUMERIC_STD.ALL;

ENTITY SHIFT_L IS 
PORT( A : IN STD_LOGIC_VECTOR ( 25 DOWNTO 0);
		B : OUT STD_LOGIC_VECTOR ( 27 DOWNTO 0);  
    
END SHIFT_L;

ARCHITECTURE EQUATIONS OF SHIFT_L IS

SIGNAL SHIFT : STD_LOGIC_VECTOR( 27 DOWNTO 0);
BEGIN
	PROCESS(A)
		BEGIN
		SHIFT  <=  '00' & A;
	END PROCESS;
	
	B <= SHIFT;
	
END EQUATIONS;



   